Files: {{ fileName }}
{% if copyrights is empty %}
Copyright: ##Hole_To_Fill##
{% else %}Copyright:{% for copyright in copyrights %} {{ copyright|replace({'\f':''}) }}
{% endfor %}{% endif %}
{% if concludedLicenses is empty %}
{% if scannerLicenses|length == 1 %}
License: {{ scannerLicenses[0] }}
{% else %}
License: ##Hole_To_Fill##
{% endif %}
{% else %}License:{% for res in concludedLicenses %} {{ res }}
{% endfor %}
{% endif %}
Comment: fileId={{ fileId }}
 .
  sha1={{ sha1 }}
  md5={{ md5 }}
{% if scannerLicenses is not empty %}
 .
{% if scannerLicenses|length == 1 %}
 License guesed by the scanners!
{% else %}
 Scanners found the following licenses:
{% for res in scannerLicenses %}
  * {{ res }}
{% endfor %}
{% endif %}
{% endif %}
