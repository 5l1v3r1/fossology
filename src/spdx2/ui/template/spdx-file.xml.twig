
<File rdf:about="{{ fileId }}">
  <fileName>{{ fileName }}</fileName>
{% if concludedLicenses is not empty %}
  <licenseConcluded>
    <DisjunctiveLicenseSet>
{% for res in concludedLicenses %}
      <member rdf:resource="{{ res }}"/>
{% endfor %}
    </DisjunctiveLicenseSet>
  </licenseConcluded>
{% endif %}
{% for res in scannerLicenses %}
  <licenseInfoInFile rdf:resource="{{ res }}" />
{% endfor %}
  <copyrightText>
{% if copyrights is empty %}
    NOASSERTION
{% else %}
  {% for cp in copyrights %}
    {{ cp }}
  {% endfor %}
{% endif %}
  </copyrightText>
</File>